@model Wall.Models.LogUser
<div class="form-wrapper">
    <h1>Login.</h1>
    <form asp-controller="Home" asp-action="Login" method="post">
        <div class="form-group">
            <label asp-for="LogEmail"></label>
            <input asp-for="LogEmail"></input>
        </div>
        <div class="form-group">
            <label asp-for="LogPassword"></label>
            <input asp-for="LogPassword"></input>
        </div>
        <button class="form-button" type="submit">Login</button>
    </form>
</div>